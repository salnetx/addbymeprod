<html lang="en">
<head>
    {{>cdnLinks}}
    <title>{{username}}</title>
</head>
<body style="font-family: 'Inter', sans-serif">
  <!-- component -->
  <div class="min-h-screen flex flex-col max-w-md mx-auto bg-gray-50 opacity-100 font-poppins px-4 bg-no-repeat bg-cover bg-center">
    <div class="flex justify-between px-1 pt-4 items-center">
      <div class="hover:bg-gray-100 rounded-full">
      <a href="/">  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24"><path fill="none" d="M0 0h24v24H0z"/><path d="M10.828 12l4.95 4.95-1.414 1.414L8 12l6.364-6.364 1.414 1.414z"/></svg>
      </a></div>
      <div>
        <p class="font-semibold">My Profile</p>
      </div>
      <div>
       <a  href="{{username}}">  <svg class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M10 12a2 2 0 100-4 2 2 0 000 4z"></path><path fill-rule="evenodd" d="M.458 10C1.732 5.943 5.522 3 10 3s8.268 2.943 9.542 7c-1.274 4.057-5.064 7-9.542 7S1.732 14.057.458 10zM14 10a4 4 0 11-8 0 4 4 0 018 0z" clip-rule="evenodd"></path></svg>
       </a></div>
    </div>
    <div class="flex items-center px-4 pt-12 justify-between">
      <div class="w-24 h-24  flex items-center rounded-full">
        <img class="h-20 w-20 rounded-full mx-auto" src="{{avatar}}">
      </div>
      <div class="w-9/12 flex items-center">
        <div class="w-10/12 flex flex-col leading-none pl-4">
          <p class="text-2xl font-bold">{{username}}</p>
          <p class="text-sm pt-1 font-light text-gray-700">{{job}} {{workplace}}</p>
          <p class="text-sm pt-2 font-light text-gray-700">"{{bio}}"</p>
        </div>
        <div class="w-2/12">
          <div>
         <a href="/edit/profile">  <svg xmlns="http://www.w3.org/2000/svg" class="text-gray-700" fill="currentColor" viewBox="0 0 24 24" width="24" height="24"><path fill="none" d="M0 0h24v24H0z"/><path d="M9.243 19H21v2H3v-4.243l9.9-9.9 4.242 4.244L9.242 19zm5.07-13.556l2.122-2.122a1 1 0 0 1 1.414 0l2.829 2.829a1 1 0 0 1 0 1.414l-2.122 2.121-4.242-4.242z"/></svg>
          </a> </div>
        </div>
      </div>
    </div>
    <div class="pt-12 px-4 w-full flex flex-col">
      <p class="font-semibold text-gray-600">Links</p>
      <div class="flex w-full pt-2 space-x-2 text-sm">
        <button class="bg-white w-28 rounded-full px-3 py-2 font-ligth text-black border border-gray-400 flex shadow"><svg class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M12.586 4.586a2 2 0 112.828 2.828l-3 3a2 2 0 01-2.828 0 1 1 0 00-1.414 1.414 4 4 0 005.656 0l3-3a4 4 0 00-5.656-5.656l-1.5 1.5a1 1 0 101.414 1.414l1.5-1.5zm-5 5a2 2 0 012.828 0 1 1 0 101.414-1.414 4 4 0 00-5.656 0l-3 3a4 4 0 105.656 5.656l1.5-1.5a1 1 0 10-1.414-1.414l-1.5 1.5a2 2 0 11-2.828-2.828l3-3z" clip-rule="evenodd"></path></svg> Links</button>
       <a href="/edit/posts"><button class="bg-black w-28 rounded-full px-3 py-2 font-ligth text-white flex"><svg class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M13.586 3.586a2 2 0 112.828 2.828l-.793.793-2.828-2.828.793-.793zM11.379 5.793L3 14.172V17h2.828l8.38-8.379-2.83-2.828z"></path></svg> Posts</button> </a>
       <a href="/edit/themes"><button class="bg-black w-28 rounded-full px-3 py-2 font-ligth text-white flex"><svg class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M4 3a2 2 0 00-2 2v10a2 2 0 002 2h12a2 2 0 002-2V5a2 2 0 00-2-2H4zm12 12H4l4-8 3 6 2-4 3 6z" clip-rule="evenodd"></path></svg> Themes</button></a>
       <a href="#"><button class="bg-black w-28 rounded-full px-3 py-2 font-ligth text-white flex"><svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" viewBox="0 0 20 20" fill="currentColor">
       <path d="M9 6a3 3 0 11-6 0 3 3 0 016 0zM17 6a3 3 0 11-6 0 3 3 0 016 0zM12.93 17c.046-.327.07-.66.07-1a6.97 6.97 0 00-1.5-4.33A5 5 0 0119 16v1h-6.07zM6 11a5 5 0 015 5v1H1v-1a5 5 0 015-5z" />
       </svg></button></a>
     
      </div>
    </div>
  
  
     <a href="/edit/sociallinks"> <p class="font-semibold px-4 text-gray-600 flex mt-5">Social links &nbsp;<svg class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm1-11a1 1 0 10-2 0v2H7a1 1 0 100 2h2v2a1 1 0 102 0v-2h2a1 1 0 100-2h-2V7z" clip-rule="evenodd"></path></svg></p>
     </a>
  
  
  
  {{>afterProfileCreation/socialButton}}
  
  
     <a href="/edit/addlinks"> <p class="font-semibold px-4 text-gray-600 flex mt-5">Added links &nbsp;<svg class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm1-11a1 1 0 10-2 0v2H7a1 1 0 100 2h2v2a1 1 0 102 0v-2h2a1 1 0 100-2h-2V7z" clip-rule="evenodd"></path></svg></p>
     </a>
  
    <a href="https://twitter.com/duolingo">
        <div class="flex justify-between items-center h-16 p-4 my-6  rounded-full border border-gray-100 shadow-xl">
        <div class="flex items-center">
          <img class="rounded-full h-12 w-12" src="https://icon-library.com/images/2018/2298785_oreos-oreo-cookie-adult-costume-hd-png-download.png" alt="Logo" />
        <div class="ml-2">
          <div class="text-sm font-semibold text-black">How to get free oreo</div>
          <div class="text-sm flex font-light text-gray-600">yourpal.com<svg class="w-3 h-3" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M11 3a1 1 0 100 2h2.586l-6.293 6.293a1 1 0 101.414 1.414L15 6.414V9a1 1 0 102 0V4a1 1 0 00-1-1h-5z"></path><path d="M5 5a2 2 0 00-2 2v8a2 2 0 002 2h8a2 2 0 002-2v-3a1 1 0 10-2 0v3H5V7h3a1 1 0 000-2H5z"></path></svg></div>
        </div>
        </div>
        <div>
      
        </div>
        </div>
      </a>
  
    
    <div class="pt-12 mb-8 px-4 w-full flex flex-col">
      <div class="flex flex-col w-full pt-2 space-y-2">
        <div class="flex w-full h-12">
          <div class="w-2/12 h-full">
            <div class="h-12 w-12  rounded-full flex items-center">
           <svg class="w-full text-blue-400" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M6.267 3.455a3.066 3.066 0 001.745-.723 3.066 3.066 0 013.976 0 3.066 3.066 0 001.745.723 3.066 3.066 0 012.812 2.812c.051.643.304 1.254.723 1.745a3.066 3.066 0 010 3.976 3.066 3.066 0 00-.723 1.745 3.066 3.066 0 01-2.812 2.812 3.066 3.066 0 00-1.745.723 3.066 3.066 0 01-3.976 0 3.066 3.066 0 00-1.745-.723 3.066 3.066 0 01-2.812-2.812 3.066 3.066 0 00-.723-1.745 3.066 3.066 0 010-3.976 3.066 3.066 0 00.723-1.745 3.066 3.066 0 012.812-2.812zm7.44 5.252a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path></svg>          </div>
          </div>
          <div class="w-8/12 h-full flex items-start">
            <p class="my-auto text-lg font-semibold">Monetize</p>
          </div>
          <div class="w-4/12 h-full flex justify-end items-end">
           <a href="/logout"> <button class="flex bg-red-700 hover:bg-red-600 hover:shadow-lg rounded-md my-auto px-5 py-1 float-right text-white font-medium">Logout</button>
           </a></div>
        </div>
      </div>
    </div>
  </div>
  <style>
  .bg-gelap{
    background-color: #001E26;
  }
  .bg-btn{
    background-color: #FF6446;
  }
  </style>
</body>
</html>