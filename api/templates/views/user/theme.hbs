     <div class="flex inline font-semibold px-4 text-gray-600 mt-5 space-x-2">
      <label for="fname">Support Button:</label><br>
     <input type="checkbox"  name="sprtbtn" {{#if sprtbtn}} checked {{/if}}
     {{#if sprtbtn}} value="0" {{else}} value="1"{{/if}}
     >
     </div>

     


     <div class=" font-semibold px-4 text-gray-600 mt-5">
     <label for="fname">Custom background:</label><br> 
     <input placeholder="Background image url" class="appearance-none block w-full  text-grey-darker border border-grey-lighter focus:outline-none rounded-lg h-10 px-4 mt-2" type="text" value="{{cstmbg}}" name="cstmbg" id="cstmbg">
     </div>

     <div class=" font-semibold px-4 text-gray-600 mt-5">
      <label for="fname">Description:</label><br> 
